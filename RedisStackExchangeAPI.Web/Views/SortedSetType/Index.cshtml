@model HashSet<string>
@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<form method="post" asp-action="Add" enctype="application/x-www-form-urlencoded">    
    <input type="text" name="name" />
    <br />
    <input type="number" name="score" />
    <button type="submit">Save</button>
</form>

@foreach (var item in Model)
{
    <h2>@item <a asp-action="DeleteItem" asp-route-name="@item">Delete</a></h2>
}
